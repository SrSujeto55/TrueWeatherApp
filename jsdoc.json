{
    "source": {
      "include": [
        "src"
      ],
      "includePattern": ".js$",
      "excludePattern": "(node_modules|docs|views)",
      "exclude": ".ejs$"
    },
    "plugins": [],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "recurse": true,
      "destination": "./docs",
      "readme": "./README.md"
    }
  }